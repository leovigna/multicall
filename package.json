{
  "name": "multicall",
  "description": "Multicall contracts",
  "version": "0.0.1",
  "homepage": "https://makerdao.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/mkrdao/multicall"
  },
  "keywords": [
    "makerdao",
    "ethereum",
    "multicall"
  ],
  "files": [
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "typescript": "^3.7.3",
    "@typechain/truffle-v5": "^2.0.0",
    "truffle": "^5.1.22",
    "typechain": "2.0.0",
    "web3": "^1.3.0",
    "web3-eth-contract": "^1.3.0",
    "@types/mocha": "^7.0.2",
    "@types/chai": "^4.2.11"
  },
  "scripts": {
    "generate-types": "typechain --target=truffle-v5 'build/contracts/*.json'",
    "postinstall": "truffle compile && yarn generate-types",
    "migrate": "tsc -p ./tsconfig.migrate.json --outDir ./migrations && truffle migrate",
    "typecheck": "tsc --noEmit -p ./tsconfig.migrate.json"
  },
  "license": "MIT"
}